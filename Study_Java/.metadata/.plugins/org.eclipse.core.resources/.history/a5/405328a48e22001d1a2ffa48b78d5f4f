package kr.brain02;

public class CarExample {

	public static void main(String[] args) {

		String str = "현대자동차";
		System.out.println(str.charAt(3));
		
		Car car = new Car();
		car.model = "택시";
		
		car.setGas("60L");
		System.out.println(car.getGas());
		//▶gas는 private라 접근을 못하는데 setGas를 이용해 접근할 수 있음
		
		System.out.println();
		car.run(2000);
		car.setGas("60L");
		
		
		
	}

}
