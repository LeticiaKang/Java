package kr.brain07;
import java.util.*;
public class PolygonExample {
	// 매개변수 배열을 사용하면 매개변수 개수를 가변적으로 사용할 수 있음
	public static void main(String[] args) {
		
		Polygon poly = new Triangle(50, 20);
		System.out.println("넓이 : " + poly.evaluate());
		System.out.println("넓이 : " + poly.evaluate());
		System.out.println("넓이 : " + poly.evaluate());
		
		
	}
}
