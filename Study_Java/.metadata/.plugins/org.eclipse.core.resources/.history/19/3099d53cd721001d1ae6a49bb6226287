package kr.brain02;

import java.util.*;

public class Test01 {
	
	private int col = 9;
	private int row = 9;
	int dan;

	// 지정한 단만 출력
	public void printOne(int dan) {
		for(int x = 1; x <= col; x++) {
			System.out.println(dan + "*" + x + "=" + dan*x);
		}
	}
	
	// 2~9단을 출력
	public void printAll(int dan) {
		for (int y = dan; y <= row; y++) {
			System.out.printf("★ %d단 입니다!\n", y);
			printOne(y);
		}
	}
	
	
	
}
